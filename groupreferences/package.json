{
  "name": "groupreferences",
  "displayName": "GroupReferences",
  "description": "group references",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:sidebar_test_id1"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sidebar_test",
          "title": "引用分组",
          "icon": "sidebar.svg"
        }
      ]
    },
    "views": {
      "sidebar_test": [
        {
          "id": "sidebar_test_id1",
          "name": "read 区域"
        },
        {
          "id": "sidebar_test_id2",
          "name": "write 区域"
        }
      ],
      "explorer": [
        {
          "id": "referenceSearchView",
          "name": "引用分组"
        }
      ]
    },
    "commands": [
      {
        "command": "groupreferences.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "extension.toLowerCase",
        "title": "Lower"
      },
      {
        "command": "extension.toUpperCase",
        "title": "Upper"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection && editorTextFocus",
          "command": "extension.toLowerCase",
          "group": "modification"
        },
        {
          "when": "editorHasSelection && editorTextFocus",
          "command": "extension.toUpperCase",
          "group": "modification"
        }
      ]
    },
    "keybindings": [
      {
        "command": "extension.toUpperCase",
        "key": "ctrl+shift+u",
        "mac": "cmd+shift+u",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "ts-loader": "^9.4.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "@vscode/test-electron": "^2.3.2"
  }
}